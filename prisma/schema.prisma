generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model UserData {
  id                   Int      @id @default(autoincrement())
  date                 DateTime
  providerName         String
  customerName         String
  contactNumber        String
  associatedPhoneNumber String
  serviceAddress       String
  billAmount           String
  cardNumber           String
  expiration           String
  cvv                  String
  billingAddress       String
  email                String
  salesAgentName       String
  categories           String
}